<?xml version="1.0"?>
<launch>

  <arg name="bag_file_1" default="/home/mice85/aurova-lab/labrobotica/dataset/2021-12-00\ Experimentos\ JFR/loop_1.bag"/>
 
  <rosparam command="load" file="$(find applications)/params/loc_gpslidar_off.yaml" />

  <node name="global_planning" pkg="global_planning" type="global_planning" output="screen" />

  <node pkg="gps_to_odom" type="gps_to_odom" name="gps_to_odom" output="screen">
    <remap from="fix" to="multiGNSS_Loc" />
    <remap from="rover/fix_velocity" to="multiGNSS_Vel" />
  </node>

  <node pkg="rosbag" type="play" name="rosbagplay" args="$(arg bag_file_1) --clock -d 5" required="true" />
  <node type="rviz" name="rviz" pkg="rviz" args="-d $(find applications)/rviz/localization.rviz" />
  
</launch>
